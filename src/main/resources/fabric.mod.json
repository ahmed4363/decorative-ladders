{
	"schemaVersion": 1,
	"id": "decorative-ladders",
	"version": "${version}",
	"name": "Decorative Ladders",
	"description": "Adds 3D ladders that use the vanilla textures, Resource Pack Support!",
	"authors": [
		"ahmed4363"
	],
	"contact": {
		"sources": "https://github.com/ahmed4363/decorative-ladders",
		"issues": "https://github.com/ahmed4363/decorative-ladders/issues"
	},
	"license": "CC0-1.0",
	"icon": "assets/decorative-ladders/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.ahmed4363.decorativeladders.DecorativeLadders"
		],
		"client": [
			"net.ahmed4363.decorativeladders.DecorativeLaddersClient"
		],
		"fabric-datagen": [
			"net.ahmed4363.decorativeladders.DecorativeLaddersDataGenerator"
		]
	},
	"mixins": [
		"decorative-ladders.mixins.json",
		{
			"config": "decorative-ladders.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.4",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}